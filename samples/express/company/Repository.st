Object subclass: #Repository	instanceVariableNames: 'items maxid'	classVariableNames: ''	poolDictionaries: ''	category: 'Repository'!"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Repository class	instanceVariableNames: ''!!Repository methodsFor: 'initialization' stamp: 'ajlopez 10/30/2013 08:50'!initialize    super initialize.    items := NativeArray new.    maxid := 0.! !!Repository methodsFor: 'adding' stamp: 'ajlopez 10/30/2013 08:50'!add: item    maxid := maxid + 1.    item nat: 'id' put: maxid.    items napply: 'push' with: { item }.! !!Repository methodsFor: 'adding' stamp: 'ajlopez 10/30/2013 08:50'!getItems    ^items napply: 'slice' with: { }.! !