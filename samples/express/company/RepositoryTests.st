Smalltalk load: 'AjUnit.st'!Object subclass: #RepositoryTests	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'RepositoryTests'!"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!RepositoryTests class	instanceVariableNames: ''!!RepositoryTests class methodsFor: 'testing' stamp: 'ajlopez 10/30/2013 08:50'!assertNew	"create Repository"    AjUnit assert: Repository new.! !!RepositoryTests class methodsFor: 'testing' stamp: 'ajlopez 10/30/2013 08:50'!assertAdd    | repo item |    repo := Repository new.    item := NativeObject new.    repo add: item.    AjUnit assert: (item nat: 'id') == 1.! !!RepositoryTests class methodsFor: 'testing' stamp: 'ajlopez 11/27/2013 19:51'!run	"run the tests in this class"	RepositoryTests assertNew.	RepositoryTests assertAdd.! !RepositoryTests run!