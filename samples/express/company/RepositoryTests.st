Smalltalk load: 'AjUnit.st'!Smalltalk load: 'Repository.st'!Object subclass: #RepositoryTests	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'RepositoryTests'!"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!RepositoryTests class	instanceVariableNames: ''!!RepositoryTests class methodsFor: 'testing' stamp: 'ajlopez 10/30/2013 08:50'!assertNew	"create Repository"    AjUnit assert: Repository new.! !!RepositoryTests class methodsFor: 'testing' stamp: 'ajlopez 10/30/2013 08:50'!assertAdd    | repo item items |    repo := Repository new.    item := NativeObject new.    repo add: item.    AjUnit assert: (item nat: 'id') == 1.    items := repo getItems.    AjUnit assert: items.    AjUnit assert: (items nat: 'length') == 1.    AjUnit assert: (items nat: 0) == item.! !!RepositoryTests class methodsFor: 'testing' stamp: 'ajlopez 10/30/2013 08:50'!assertGetById    | repo item newitem |    repo := Repository new.    item := NativeObject new.    repo add: item.    newitem := repo getById: 1.    AjUnit assert: newitem.    AjUnit assert: newitem == item.! !!RepositoryTests class methodsFor: 'testing' stamp: 'ajlopez 11/27/2013 19:51'!run	"run the tests in this class"	RepositoryTests assertNew.	RepositoryTests assertAdd.	RepositoryTests assertGetById.! !RepositoryTests run!